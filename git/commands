# создать .gitignore

# склонить удаленный репозиторий
git clone {url репозитория}

# подтянуть изменения
git pull

# посмотреть доступные ветки
git branch

# перейти на другую ветку
git checkout {название ветки}

# создать ветку и сразу перейти в нее
git checkout -b {название новой ветки}

# отменить изменения в файле
git checkout -- {имя измененного файла}

# удалить файл из git, но не локально
git rm --cache {имя файла}

# удалить папку из git, но не локально
git rm -r --cache {имя папки}